<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="17dp">

    <TextView
        android:id="@+id/tv_item_xun_yi_consult_time_right_photo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="3dp"
        android:textColor="#999999"
        android:textSize="11dp" />


    <RelativeLayout
        android:id="@+id/tv_item_xun_yi_consult_photo_rl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/tv_item_xun_yi_consult_time_right_photo"
        android:layout_marginRight="12dp">

        <ImageView
            android:id="@+id/tv_item_xun_yi_consult_photo"
            android:layout_width="112dp"
            android:layout_height="120dp"
            android:scaleType="fitXY"
            />

        <ImageView
            android:layout_width="112dp"
            android:layout_height="120dp"
            android:scaleType="fitXY"
            android:src="@mipmap/rc_xun_yi_img_frame" />

    </RelativeLayout>
    <TextView
        android:id="@+id/tv_item_xun_yi_consult_photo_finish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:visibility="gone"
        android:text="@string/rc_xun_yi_finish"
        android:layout_below="@+id/tv_item_xun_yi_consult_photo_rl"
        android:layout_marginBottom="17dp"
        android:textColor="#999999"
        android:textSize="11dp" />
</RelativeLayout>