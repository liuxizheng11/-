<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.RecyclerView
        android:id="@+id/activity_f_d_i_recyclerview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/activity_f_d_i_button" />

    <TextView
        android:id="@+id/activity_f_d_i_button"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="11dp"
        android:layout_marginRight="11dp"
        android:layout_marginTop="10dp"
        android:background="?attr/RCDarkColor"
        android:gravity="center"
        android:textSize="@dimen/rc_text_size"
        android:text="@string/rc_fd_video_consult"
        android:textColor="@color/rc_white" />


</RelativeLayout>
